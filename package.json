{
  "name": "probate-caveats-frontend",
  "description": "Web app to check if an applicant is eligible to apply for caveats online",
  "version": "0.0.1",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "setup": "NODE_PATH=. npm run sass && npm run git-info && npm run sass-ie8",
    "start": "NODE_PATH=. node server.js",
    "debug": "NODE_PATH=. node inspect server.js",
    "prestart:dev": "npm run sass:watch",
    "start:dev": "NODE_PATH=. nodemon -e js,json --watch 'app/' --watch 'test/' -x 'node server.js'",
    "sass:watch": "NODE_PATH=. watch 'npm run -s sass' app/assets/sass/ &",
    "sass": "NODE_PATH=. node-sass app/assets/sass/application.scss --output public/stylesheets --quiet --output-style expanded --source-map true --include-path node_modules/govuk_template_jinja/assets/stylesheets --include-path node_modules/govuk_frontend_toolkit/stylesheets --include-path node_modules/govuk-elements-sass/public/sass/",
    "sass-ie8": "NODE_PATH=. node-sass app/assets/sass/application-ie8.scss --output public/stylesheets --quiet --output-style expanded --source-map true --include-path node_modules/govuk_template_jinja/assets/stylesheets --include-path node_modules/govuk_frontend_toolkit/stylesheets --include-path node_modules/govuk-elements-sass/public/sass/",
    "test": "npm run test-unit -- $@ && npm run test-accessibility -- $@ && npm run test-component -- $@ && npm run test-route -- $@",
    "test-unit": "NODE_PATH=. LOG_LEVEL=error mocha test/unit --reporter spec --recursive --timeout 15000",
    "test-component": "NODE_PATH=. LOG_LEVEL=error mocha test/component/ --reporter spec --recursive --timeout 15000",
    "test-route": "NODE_PATH=. LOG_LEVEL=error mocha test/route --reporter spec --recursive --timeout 15000",
    "test:a11y": "NODE_PATH=. LOG_LEVEL=error ENABLE_TRACKING=false mocha test/accessibility --timeout 15000",
    "test-accessibility": "NODE_PATH=. LOG_LEVEL=error ENABLE_TRACKING=false mocha test/accessibility --timeout 15000",
    "test-e2e": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/ --steps --reporter mochawesome",
    "test-crossbrowser-e2e": "NODE_PATH=. codeceptjs run -c test/end-to-end/saucelabs.conf.js --steps",
    "test:smoke": "mocha test/smoke/healthcheck.js --timeout 15000 --reporter spec",
    "lint": "NODE_PATH=. eslint .",
    "eslint": "NODE_PATH=. eslint .",
    "eslint-dev:all": "NODE_PATH=. eslint . -c .eslintrc-dev.js",
    "eslint-dev:file": "NODE_PATH=. eslint -c .eslintrc-dev.js",
    "test:nsp": "NODE_PATH=. nsp check",
    "security": "NODE_PATH=. nsp check",
    "test:functional": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/ --steps --reporter mochawesome",
    "test:fullfunctional": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/ --steps --reporter mochawesome",
    "test:codecov-upload": "NODE_ENV=test  NODE_PATH=. istanbul cover _mocha './test/**/test*.js' -- -R spec && codecov",
    "test:coverage": "NODE_ENV=test NODE_PATH=. istanbul cover _mocha './test/**/test*.js'",
    "test:stryker": "NODE_PATH=. stryker run ",
    "test:mutation": "NODE_PATH=. stryker run test/mutation/mutation_unittests.conf.js",
    "test:mutation-shell": "./bin/run-mutation-tests.sh",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner -Dsonar.projectName='Probate :: caveats-fe' -Dsonar.sources=app -Dsonar.cpd.exclusions=app/steps/** -Dsonar.language=js -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info",
    "git-info": "node-git-info-json",
    "//": "The below scripts are just stubs, don't use them in production",
    "orchestration-stub": "NODE_PATH=. node test/service-stubs/orchestration.js",
    "s2s-stub": "NODE_PATH=. node test/service-stubs/s2s.js",
    "idam-stub": "NODE_PATH=. node test/service-stubs/idam.js",
    "find-address-stub": "NODE_PATH=. node test/service-stubs/find-address.js"
  },
  "dependencies": {
    "@hmcts/nodejs-logging": "^3.0.0",
    "ajv": "^4.11.0",
    "applicationinsights": "^1.0.2",
    "body-parser": "^1.18.3",
    "co": "^4.6.0",
    "codacy-coverage": "^2.1.1",
    "codecov": "^3.0.0",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dateformat": "^3.0.3",
    "debug": "^3.1.0",
    "express": "^4.14.1",
    "express-nunjucks": "^2.2.3",
    "express-session": "^1.13.0",
    "file-type": "^10.4.0",
    "govuk-elements-sass": "3.1.3",
    "govuk_frontend_toolkit": "7.1.0",
    "govuk_template_jinja": "0.23.0",
    "helmet": "3.8.2",
    "https-proxy-agent": "^2.2.1",
    "i18next": "^10.0.0",
    "ioredis": "^3.2.2",
    "jquery": "^3.3.1",
    "jsdom": "^12.2.0",
    "lodash": "^4.17.5",
    "mocha-lcov-reporter": "^1.3.0",
    "multer": "^1.4.1",
    "node-fetch": "^2.2.0",
    "node-git-info-json": "^0.1.1",
    "node-sass": "^4.6.1",
    "numeral": "^2.0.6",
    "nunjucks": "^3.0.1",
    "otp": "^0.1.3",
    "randomstring": "^1.1.5",
    "request": "^2.88.0",
    "require-directory": "^2.1.1",
    "serve-favicon": "2.4.5",
    "stryker": "^0.23.0",
    "uuid": "^3.3.2",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.0.0",
    "codeceptjs": "^1.4.6",
    "eslint": "4.1.1 ",
    "eslint-plugin-mocha": "^4.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "mocha-jenkins-reporter": "^0.4.1",
    "mochawesome": "^2.0.0",
    "nock": "^9.3.0",
    "nodemon": "^1.17.5",
    "nsp": "^3.2.1",
    "pa11y": "^4.13.2",
    "phantomjs-prebuilt": "^2.1.13",
    "puppeteer": "^1.4.0",
    "rewire": "^4.0.1",
    "selenium-standalone": "^5.7.2",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "sonar-scanner": "3.0.3",
    "sonarqube-scanner": "^1.1.0",
    "stryker-api": "^0.16.1",
    "stryker-html-reporter": "^0.13.2",
    "stryker-javascript-mutator": "^0.6.3",
    "stryker-mocha-framework": "^0.9.2",
    "stryker-mocha-runner": "^0.11.2",
    "superagent": "^3.8.2",
    "supertest": "^2.0.1",
    "watch": "^1.0.1",
    "when": "^3.7.7"
  }
}
